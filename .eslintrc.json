{
    "root": true,
    
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "jquery": true
      },
    "plugins": [
        "html"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": [
            "error",
            {
              "endOfLine": "auto"
            }     
        ],
        "no-unused-vars": "off"
    },
    "globals": {
      "document": true,
      "window": true
  }
}